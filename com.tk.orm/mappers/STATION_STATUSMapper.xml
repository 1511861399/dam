<?xml version="1.0" encoding="UTF-8" ?> 
<sqlMap namespace="EntityModel" xmlns="http://ibatis.apache.org/mapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"> 
    <alias>
		<!--类的别名-->
		<typeAlias alias="STATION_STATUS" type="com.tk.orm.model.STATION_STATUS,com.tk.orm"/>
	</alias>
    <parameterMaps>
	<parameterMap id="STATION_STATUS_params">
    			<parameter property="ID" column="ID" type="int" dbType="int" ></parameter>
    			<parameter property="STATION_ID" column="STATION_ID" type="int" dbType="int" ></parameter>
    			<parameter property="LINKSTATUS" column="LINKSTATUS" type="byte" dbType="tinyint" ></parameter>
    			<parameter property="ALARMSTATUS" column="ALARMSTATUS" type="byte" dbType="tinyint" ></parameter>
    			<parameter property="QUITSTATUS" column="QUITSTATUS" type="byte" dbType="tinyint" ></parameter>
    	</parameterMap>
    </parameterMaps>
    <resultMaps>
        <resultMap id="STATION_STATUSResult" class="STATION_STATUS"> 
            <result property="ID" column="ID"/>
            <result property="STATION_ID" column="STATION_ID"/>
            <result property="LINKSTATUS" column="LINKSTATUS"/>
            <result property="ALARMSTATUS" column="ALARMSTATUS"/>
            <result property="QUITSTATUS" column="QUITSTATUS"/>
        </resultMap>
    </resultMaps>
    <statements>
    <!--查询所有记录-->
		<select id="SelectAllSTATION_STATUS" resultMap="STATION_STATUSResult">
			<![CDATA[SELECT  
             ID,
    	     
             STATION_ID,
    	     
             LINKSTATUS,
    	     
             ALARMSTATUS,
    	     
             QUITSTATUS
    	     FROM T_STATION_STATUS]]>
		</select>
        <!--查询单条记录-->
		<select id="SelectSTATION_STATUSById" parameterClass="int" resultMap="STATION_STATUSResult" extends="SelectAllSTATION_STATUS">
			<![CDATA[ where ID = #value#  ]]>
		</select> 
        
        
        
        <!--插入新记录-->
		<insert id="InsertSTATION_STATUS" parameterClass="STATION_STATUS">	
          select 1;
    			<!--id自动生成的示例用法-->
          <selectKey property="ID" type="post" resultClass="int">
          <![CDATA[
            INSERT into T_STATION_STATUS(STATION_ID, LINKSTATUS, ALARMSTATUS, QUITSTATUS )
    			  VALUES(#STATION_ID#, #LINKSTATUS#, #ALARMSTATUS#, #QUITSTATUS# )
            SELECT SCOPE_IDENTITY() as ID
          ]]>
          </selectKey>
      
		</insert>
        
        <!--更新单条记录-->
		<update id="UpdateSTATION_STATUS" parameterClass="STATION_STATUS">
			<![CDATA[Update T_STATION_STATUS 
            SET  
                    STATION_ID=#STATION_ID#
                , 
                    LINKSTATUS=#LINKSTATUS#
                , 
                    ALARMSTATUS=#ALARMSTATUS#
                , 
                    QUITSTATUS=#QUITSTATUS#
                 
			Where ID=#ID#]]>
		</update>

		<!--根据主键删除单条记录-->
		<delete id="DeleteSTATION_STATUSById" parameterClass="int">
			<![CDATA[Delete From T_STATION_STATUS Where ID=#value#]]>
		</delete>
        <select id="SelectSTATION_STATUSQuery" parameterMap="STATION_STATUS_params" resultMap="STATION_STATUSResult">
    
                select
            		a.ID,
            		a.STATION_ID,
            		a.LINKSTATUS,
            		a.ALARMSTATUS,
            		a.QUITSTATUS,	
        	from T_STATION_STATUS a 
        	<dynamic prepend="where">
        		<isPropertyAvailable prepend="" property="ID">
        			<isGreaterThan prepend="and" property="ID" compareValue="0">
        				a.ID = #ID#
        			</isGreaterThan>
        		</isPropertyAvailable>
        	</dynamic>
        	<!--order by-->
        	<dynamic>
        	order by
        	a.ID
        	</dynamic>
        </select> 
        
       
    </statements>
</sqlMap>  
